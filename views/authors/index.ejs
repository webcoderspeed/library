<h2>Search Author</h2>
<form action="/authors" method="GET">
  <label>Name</label>
  <input type="text" name="name" value="<%= searchOptions.name %> ">
  <button type="submit">Search</button>
</form>
<br>
<% authors.forEach(author => { %> 
  <div><span>ID: <%= author.id %></span> <%= author.name %> </div>
<% }) %> 